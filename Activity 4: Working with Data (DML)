CREATE TABLE students (
    student_id NUMBER,
    name VARCHAR2(50),
    grade NUMBER,
    favorite_subject VARCHAR2(50)
);


INSERT INTO students (student_id, name, grade, favorite_subject)
VALUES (1, 'Alex', 6, 'Math');

