DECLARE
   movie_title   VARCHAR2(50) := 'Inception';
   director_name VARCHAR2(50) := 'Christopher Nolan';
   genre          VARCHAR2(30) := 'Science Fiction';
   times_watched  NUMBER := 5;
BEGIN
   DBMS_OUTPUT.PUT_LINE('Movie Details:');
   DBMS_OUTPUT.PUT_LINE('Title: ' || movie_title);
   DBMS_OUTPUT.PUT_LINE('Director: ' || director_name);
   DBMS_OUTPUT.PUT_LINE('Genre: ' || genre);
   DBMS_OUTPUT.PUT_LINE('Times Watched: ' || times_watched);
END;
/
